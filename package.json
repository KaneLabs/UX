  "repository": {
    "type": "git",
    "url": "git://github.com/kanelabs/eros-ui.git"
  },
  "scripts": {
    "dev": "concurrently \"yarn start\" \"yarn storybook\" \"yarn ios\"",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "install:pod": "cd ios && pod install && cd ../",
    "reset": "yarn clean && yarn && yarn build && yarn dev",
    "compile:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist/cjs --ignore \"**/__tests__\"",
    "compile:es": "babel src --out-dir dist --ignore \"**/*.test.js\"",
    "test": "cross-env NODE_ENV=test jest --testMatch \"<rootDir>/src/**/*.test.js\" --coverage",
    "test:ci": "cross-env NODE_ENV=test jest --testMatch \"<rootDir>/lib/**/*.test.js\" --coverage --ci -u",
    "test:watch": "cross-env NODE_ENV=test jest --testMatch \"<rootDir>/src/**/*.test.js\" --watch",
    "test:integration": "echo \"No Integration Tests\"",
    "test:e2e": "echo \"No E2E Tests\"",
    "lint": "eslint src",
    "web": "yarn storybook",
    "storybook": "start-storybook start -p 7007",
    "storybook:build": "build-storybook",
    "clean": "watchman watch-del-all && rm -rf node_modules && rm -rf dist && rm -rf lib",
    "build": "yarn build:babel",
    "build:babel": "npx babel src --out-dir lib && cp -R src/assets lib/assets",
    "build:webpack": "webpack"
  },
